{
    "name": "silvanite/nova-field-cloudinary",
    "description": "A Laravel Nova Image Field with Flysystem Adapter for storing and retrieving media from Cloudinary",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "cloudinary",
        "media",
        "images"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Mark",
            "email": "m2de@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.0.0",
        "laravel/nova": "^4.12",
        "carlosocarvalho/flysystem-cloudinary": "^2.0",
        "davidpiesse/nova-audio": "^0.0.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": ">=3.1"
    },
    "autoload": {
        "files": [
            "src-php/helpers.php"
        ],
        "psr-4": {
            "Silvanite\\NovaFieldCloudinary\\": "src-php/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra":{
        "laravel": {
            "providers": [
                "Silvanite\\NovaFieldCloudinary\\Providers\\PackageServiceProvider"
            ]
        }
    }
}
